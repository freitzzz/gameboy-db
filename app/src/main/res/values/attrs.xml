<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="label" format="reference|string" />
    <attr name="value" format="reference|string|boolean" />
    <attr name="icon" format="reference" />
    <attr name="editable" format="boolean" />
    <attr name="checked" format="boolean" />

    <declare-styleable name="ValuesPreference">
        <attr name="label" />
        <attr name="value" />
        <attr name="icon" />
        <attr name="editable" />
    </declare-styleable>

    <declare-styleable name="SwitchPreference">
        <attr name="label" />
        <attr name="value" />
        <attr name="icon" />
    </declare-styleable>

    <declare-styleable name="CheckedListItem">
        <attr name="value"/>
        <attr name="checked"/>
    </declare-styleable>

    <declare-styleable name="NavigableTopBar">
        <attr name="title" format="string"/>
        <attr name="primaryActionIcon" format="reference"/>
        <attr name="secondaryActionIcon" format="reference"/>
    </declare-styleable>
</resources>